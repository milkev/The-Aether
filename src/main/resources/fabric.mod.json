    "issues": "https://github.com/devs-immortal/The-Aether/issues"
  },
  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": ">=1.17",
    "java": ">=16",
    "fabric-api-base": "*",
    "fabric-registry-sync-v0": "*",
    "fabric-networking-v0": "*",
    "trinkets": "*"
  },
  "mixins": [
    "the_aether.mixins.json"
